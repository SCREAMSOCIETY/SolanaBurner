{
  "version": 2,
  "builds": [
    {
      "src": "fastifyServer.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": [
          "static/**",
          "helius-api.js",
          "cnft-burn-server.js",
          "cnft-transfer-server.js",
          "server-transfer.js",
          "queue-transfer-manager.js",
          "delegated-cnft-transfer.js",
          "config.js"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/fastifyServer.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
